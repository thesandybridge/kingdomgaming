{
    "$schema": "https://schemas.wp.org/trunk/theme.json",
    "version": 2,
    "title": "Kingdom Gaming",
    "settings": {
        "appearanceTools": true,
        "color": {
            "defaultPalette": false,
            "link": true,
            "background": true,
            "palette": [
                {
                    "name": "Black",
                    "slug": "black",
                    "color": "#000000"
                },
                {
                    "name": "White",
                    "slug": "white",
                    "color": "#ffffff"
                },
                {
                    "name": "Kingdom Midnight",
                    "slug": "kingdom-midnight",
                    "color": "#050505"
                },
                {
                    "name": "Kingdom Gold",
                    "slug": "kingdom-gold",
                    "color": "#bfa25d"
                },
                {
                    "name": "Kingdom Red",
                    "slug": "kindgom-red",
                    "color": "#ff6b6b"
                },
                {
                    "name": "Kingdom Salmon",
                    "slug": "kindgom-salmon",
                    "color": "#c96480"
                },
                {
                    "name": "Kingdom Indigo",
                    "slug": "kindgom-indigo",
                    "color": "#89aae6"
                },
                {
                    "name": "Kindgom Blue",
                    "slug": "kindgom-blue",
                    "color": "#9ae5e6"
                },
                {
                    "name": "Kindgom Navy",
                    "slug": "kindgom-navy",
                    "color": "#020a1d"
                },
                {
                    "name": "Kindgom Charcoal",
                    "slug": "kindgom-charcoal",
                    "color": "#101010"
                }
            ]
        },
        "blocks": {
                "core/columns": {
                    "spacing": {
                        "margin": true,
                        "padding": true
                    }
                }
        },
        "layout": {
            "contentSize": "900px",
            "wideSize": "1400px"
        },
        "spacing": {
            "margin": true,
            "padding": true,
            "blockGap": true,
            "units": [ "px", "em", "rem", "vh", "vw" ]
        },
        "typography": {
            "customFontSize": true,
            "lineHeight": true,
            "dropCap": true,
            "fluid": false,
            "fontStyle": true,
            "fontWeight": true,
            "letterSpacing": true,
            "textDecoration": true,
            "textTransform": true,
            "fontSizes": [
                {
                    "name": "Small",
                    "slug": "small",
                    "size": "0.75rem"
                },
                {
                    "name": "Medium",
                    "slug": "medium",
                    "size": "1rem"
                }
            ]
        }
    }
}

